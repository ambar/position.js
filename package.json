{
  "name": "position.js",
  "version": "0.2.4",
  "description": "Positions a popup element to an anchor element.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "es",
    "lib",
    "src"
  ],
  "scripts": {
    "start": "BABEL_ENV=examples node examples/server.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "npm test -- --watch",
    "build:commonjs": "rm -rf ./lib && NODE_ENV=production BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "rm -rf ./es && NODE_ENV=production BABEL_ENV=es babel src --out-dir es",
    "build": "npm run build:commonjs && npm run build:es",
    "build:examples": "rm -rf ./examples/dist && BABEL_ENV=examples NODE_ENV=production webpack --config ./examples/webpack.config.js",
    "deploy:examples": "gh-pages -o github -d ./examples/dist",
    "prepublish": "npm run eslint && npm test && npm run build",
    "eslint": "eslint src"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ambar/position.js.git"
  },
  "keywords": [
    "position",
    "dom",
    "placement",
    "alignment",
    "viewport",
    "popup",
    "popover",
    "tooltip",
    "overlay"
  ],
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.7.1",
    "eslint-config-zhihu": "^0.2.2",
    "gh-pages": "^1.0.0",
    "jest": "^22.0.0"
  },
  "dependencies": {}
}
